{
	"name": "matteo-girardi.github.io",
	"version": "9.0.0",
	"description": "Matteo Girardi's website, using the Eleventy site generator and based on eleventy-base-blog",
	"type": "module",
	"scripts": {
		"build": "npx @11ty/eleventy",
		"build-nocolor": "cross-env NODE_DISABLE_COLORS=1 npx @11ty/eleventy",
		"build-ghpages": "npx @11ty/eleventy",
		"start": "npx @11ty/eleventy --serve --quiet",
		"start-ghpages": "npx @11ty/eleventy --serve --quiet",
		"debug": "cross-env DEBUG=Eleventy* npx @11ty/eleventy",
		"debugstart": "cross-env DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "cross-env DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/matteo-girardi/matteo-girardi.github.io"
	},
	"author": {
		"name": "Matteo Girardi",
		"email": "",
		"url": "https://matteo-girardi.github.io/"
	},
	"license": "MIT",
	"engines": {
		"node": ">=18"
	},
	"funding": {
		"type": "opencollective",
		"url": "https://opencollective.com/11ty"
	},
	"bugs": {
		"url": "https://github.com/matteo-girardi/matteo-girardi.github.io/issues"
	},
	"homepage": "https://matteo-girardi.github.io/",
	"devDependencies": {
		"@11ty/eleventy": "^3.1.2",
		"@11ty/eleventy-img": "^6.0.4",
		"@11ty/eleventy-navigation": "^1.0.4",
		"@11ty/eleventy-plugin-rss": "^2.0.4",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.1",
		"cross-env": "^10.0.0",
		"luxon": "^3.5.0",
		"zod": "^4.0.14",
		"zod-validation-error": "^4.0.1"
	},
	"dependencies": {
		"@zachleat/heading-anchors": "^1.0.3",
		"bootstrap-icons": "^1.13.1"
	}
}
